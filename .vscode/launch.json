    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Fingerprint with output",
            "program": "${workspaceRoot}/cli.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "index.html", "-o out.html"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Fingerprint",
            "program": "${workspaceRoot}/cli.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "index.html"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Fingerprint with glob",
            "program": "${workspaceRoot}/cli.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "*.html", "-o out/"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Fingerprint with non-existant file",
            "program": "${workspaceRoot}/cli.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "non.html", "-o out/"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Tests",
            "program": "${workspaceRoot}/node_modules/mocha/bin/mocha",
            "cwd": "${workspaceRoot}",
            "args": [
                "--debug"
            ]
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858
        }
    ]
}